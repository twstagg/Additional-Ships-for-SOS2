<Defs>



	<ThingDef ParentName="AS_ShipPartBase">
		<defName>AS_PurpleShipChunkBottom</defName>
		<label>bioship, bottom</label>
		<description>An experimental organic bioship. These ships are artificially grown around a metallic frame, and are quite resilient to damage.</description>		
		<graphicData>
			<texPath>Things/Building/AS_PurpleShipChunkBottom</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			 
			<drawSize>(21,21)</drawSize>
		</graphicData>
		<designatorDropdown>AS_PurpleBioship</designatorDropdown>
		<costList>
			<Steel>50</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<uiIconScale>0.5</uiIconScale>  
		<size>(15,21)</size>	
	</ThingDef>

