<?xml version="1.0" encoding="utf-8" ?>
<Defs>



	<ThingDef ParentName="AS_ShipPartBase">
		<defName>AS_GreyShipChunkTop</defName>
		<label>pirate ship, top</label>
		<description>Pirate ships often prey on unsuspecting convoy and trade ships. Fast and agile, they are not designed for prolonged combat, but for hit and run assaults.</description>		
		<graphicData>
			<texPath>Things/Building/AS_GreyShipChunkTop</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			 
			<drawSize>(7,7)</drawSize>
		</graphicData>
		<designatorDropdown>AS_GreyShip</designatorDropdown>
		<costList>
			<Steel>50</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<uiIconScale>0.5</uiIconScale>  
		<size>(7,6)</size>	
	</ThingDef>


